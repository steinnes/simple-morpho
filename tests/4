fun test(a,b)
{
        if (a || b)
        {
                writeln("if-1 or-1");
                if (a && b)
                {
                        writeln("if-2 and-2");
                };
        };
}

fun main()
{
	writeln("1 and 2");
	test(true,true);
	writeln("1");
	test(true,false);
	writeln("..none");
	test(false,false);
	writeln("done!");
}
